{
  "name": "@rectangulr/rectangulr",
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "build": "wireit",
    "dev": "ng build --watch",
    "test": "wireit",
    "test:build": "ng test",
    "test:watch": "wireit",
    "publish": "bash dev/publish.sh"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rectangulr/rectangulr.git"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/@rectangulr"
  },
  "wireit": {
    "build": {
      "command": "bash dev/dist.sh",
      "dependencies": [
        "build:angular"
      ]
    },
    "build:angular": {
      "command": "ng build --configuration production"
    },
    "test": {
      "command": "node dist-test/main.cjs",
      "dependencies": [
        "test:build"
      ]
    },
    "test:watch": {
      "command": "ng test --watch",
      "service": true
    }
  },
  "dependencies": {
    "@manaflair/term-strings": "^0.9.2",
    "@s-libs/micro-dash": "^16.1.0",
    "cli-truncate": "^3.1.0",
    "ng-dynamic-component": "^10.5.1",
    "tslib": "^2.0.0",
    "typeflex": "^0.1.0",
    "widest-line": "^4.0.1",
    "wrap-ansi": "^9.0.0"
  },
  "peerDependencies": {
    "chai": "^4.3.7",
    "core-decorators": "^0.13.0",
    "fuse.js": "^6.4.6",
    "json5": "^2.2.0",
    "lodash": "^4.17.21",
    "rxjs": "^7.8.1"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "^17.0.0",
    "@angular-devkit/build-angular": "^17.0.7",
    "@angular/cli": "^17.0.7",
    "@angular/common": "^17.0.6",
    "@angular/compiler": "^17.0.6",
    "@angular/compiler-cli": "^17.0.6",
    "@angular/core": "^17.0.6",
    "@angular/forms": "^17.0.6",
    "@angular/platform-browser": "^17.0.6",
    "@angular/platform-browser-dynamic": "^17.0.6",
    "@babel/cli": "^7.19.3",
    "@changesets/cli": "^2.27.1",
    "@types/jasmine": "^4.3.1",
    "@types/lodash": "^4.14.189",
    "@types/node": "^14.0.0",
    "@types/webpack-env": "^1.18.0",
    "@typescript-eslint/eslint-plugin": "^5.44.0",
    "@typescript-eslint/parser": "^5.44.0",
    "core-decorators": "^0.13.0",
    "eslint": "^8.28.0",
    "eslint-plugin-capture": "^1.1.0",
    "fuse.js": "^6.4.6",
    "jasmine": "^4.5.0",
    "json5": "^2.2.0",
    "lodash": "^4.17.21",
    "ng-dynamic-component": "^10.5.1",
    "ng-packagr": "^17.0.2",
    "rollup": "^4.9.0",
    "typescript": "^5.2.0",
    "wireit": "^0.14.0",
    "zone.js": "^0.14.2"
  }
}