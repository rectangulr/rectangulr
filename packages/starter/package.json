{
	"name": "starter",
	"version": "0.0.0",
	"type": "module",
	"private": true,
	"scripts": {
		"build": "wireit",
		"test": "node dist/main.js"
	},
	"wireit": {
		"build": {
			"command": "bash dev/dist.sh",
			"dependencies": [
				"build:angular"
			]
		},
		"build:angular": {
			"command": "ng build",
			"files": [
				"src/**",
				"angular.json"
			],
			"output": [
				"dist-angular"
			]
		}
	},
	"dependencies": {
		"@angular/common": "^17.0.6",
		"@angular/compiler": "^17.0.6",
		"@angular/compiler-cli": "^17.0.6",
		"@angular/core": "^17.0.6",
		"@angular/forms": "^17.0.6",
		"@angular/platform-browser": "^17.0.6",
		"@manaflair/term-strings": "^0.9.2",
		"core-decorators": "^0.13.0",
		"fuse.js": "^6.4.6",
		"json5": "^2.2.0",
		"@rectangulr/rectangulr": "*",
		"rxjs": "^7.8.1",
		"tslib": "^2.0.0",
		"zone.js": "^0.14.2"
	},
	"devDependencies": {
		"@angular-builders/custom-webpack": "^17.0.0",
		"@angular/cli": "^17.0.7",
		"@types/node": "^18.0.0",
		"@tsconfig/node14": "^1.0.2",
		"typescript": "^5.2.0",
		"wireit": "^0.14.0"
	}
}