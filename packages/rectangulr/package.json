{
  "name": "@rectangulr/rectangulr",
  "version": "0.0.7",
  "type": "module",
  "scripts": {
    "build": "wireit",
    "dev": "ng build --watch",
    "test": "wireit",
    "test:build": "ng test",
    "test:watch": "wireit",
    "publish": "bash dev/publish.sh"
  },
  "module": "dist/fesm2022/rectangulr-rectangulr.mjs",
  "typings": "dist/index.d.ts",
